# FunÃ§Ã£o para instalar Python3 baseado na versÃ£o do sistema
install_python3() {
    if command -v python3 >/dev/null 2>&1 && dpkg-query -W -f='${Status}' socat 2>/dev/null | grep -q "ok installed"; then
        echo "Python3 e socat jÃ¡ estÃ£o instalados."
    else
        . /etc/os-release
        if { [[ "$ID" == "ubuntu" && "$VERSION_ID" == "20.04" ]] || [[ "$ID" == "debian" && "$VERSION_ID" == "11" ]]; }; then
            apt-get install -y python3.8 python3-pip socat >/dev/null 2>&1 || {
                echo -e "\033[1;31mErro ao instalar Python3.8 ou socat\033[0m"
                exit 1
            }
            update-alternatives --install /usr/bin/python3 python3 /usr/bin/python3.8 1 >/dev/null 2>&1
            update-alternatives --set python3 /usr/bin/python3.8 >/dev/null 2>&1
        elif { [[ "$ID" == "ubuntu" && "$VERSION_ID" == "22.04" ]] || [[ "$ID" == "debian" && "$VERSION_ID" == "12" ]]; }; then
            apt-get install -y python3.10 python3-pip socat >/dev/null 2>&1 || {
                echo -e "\033[1;31mErro ao instalar Python3.10 ou socat\033[0m"
                exit 1
            }
            update-alternatives --install /usr/bin/python3 python3 /usr/bin/python3.10 1 >/dev/null 2>&1
            update-alternatives --set python3 /usr/bin/python3.10 >/dev/null 2>&1
        else
            echo -e "\033[1;31m[SISTEMA NÃƒO SUPORTADO] Apenas Ubuntu 20.04, Ubuntu 22.04, Debian 11 e Debian 12 sÃ£o compatÃ­veis.\033[0m"
            exit 1
        fi
    fi
}

# Chama a funÃ§Ã£o de instalaÃ§Ã£o do Python3 com barra de progresso
fun_bar "install_python3"

