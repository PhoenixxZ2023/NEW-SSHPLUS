#!/bin/bash
clear

# FunÃ§Ã£o para verificar e instalar pacotes
install_if_missing() {
    local pkg="$1"
    if ! dpkg-query -W -f='${Status}' "$pkg" 2>/dev/null | grep -q "ok installed"; then
        echo "Instalando $pkg..."
        apt-get install -y "$pkg" >/dev/null 2>&1 || {
            echo -e "\033[1;31mErro ao instalar $pkg\033[0m"
            exit 1
        }
    else
        echo "$pkg jÃ¡ estÃ¡ instalado."
    fi
}

# FunÃ§Ã£o para instalar pacotes iniciais
install_initial_packages() {
    echo "Instalando pacotes iniciais..."
    apt-get install -y lolcat iproute2 toilet curl git figlet boxes net-tools procps sysstat >/dev/null 2>&1 || {
        echo -e "\033[1;31mErro ao instalar pacotes iniciais\033[0m"
        exit 1
    }
}

# InstalaÃ§Ã£o silenciosa de pacotes
install_initial_packages

# VerificaÃ§Ã£o se o usuÃ¡rio Ã© root
cd $HOME
if [[ "$(whoami)" != "root" ]]; then
    echo -e "\033[1;33m[\033[1;31mErro\033[1;33m] \033[1;37m- \033[1;33mVOCÃŠ PRECISA EXECUTAR A INSTALAÃ‡ÃƒO DO SCRIPT COMO USUÃRIO ROOT\033[0m"
    rm $HOME/Plus >/dev/null 2>&1
    exit 1
fi

# FunÃ§Ã£o barra de progresso com limpeza robusta
fun_bar() {
    comando[0]="$1"
    comando[1]="$2"
    # Criar arquivo temporÃ¡rio Ãºnico usando mktemp
    local temp_file
    temp_file=$(mktemp "$HOME/fim.XXXXXX") || {
        echo -e "\033[1;31mErro ao criar arquivo temporÃ¡rio\033[0m"
        exit 1
    }
    # Armadilha para remover o arquivo temporÃ¡rio em caso de interrupÃ§Ã£o
    trap 'rm -f "$temp_file" >/dev/null 2>&1' EXIT INT TERM
    (
        # Executar comandos e criar arquivo temporÃ¡rio
        ${comando[0]} -y >/dev/null 2>&1
        ${comando[1]} -y >/dev/null 2>&1
        touch "$temp_file" >/dev/null 2>&1
    ) >/dev/null 2>&1 &
    tput civis
    echo -ne    "\033[1;31m   âª\033[1;33mAGUARDE \033[5;37m>\033[5;37m>\033[5;37m>\033[5;37m> \033[1;93m["
    while true; do
        for ((i = 0; i < 18; i++)); do
            echo -ne "\033[1;92mâ– "
            sleep 0.1s
        done
        if [[ -e "$temp_file" ]]; then
            rm -f "$temp_file" >/dev/null 2>&1 && break
        fi
        echo -e "\033[1;93m]"
        sleep 1s
        tput cuu1
        tput dl1
        echo -ne "\033[1;31m   âª\033[1;33mAGUARDE \033[5;37m>\033[5;37m>\033[5;37m>\033[5;37m> \033[1;93m["
    done
    echo -e "\033[1;33m]\033[1;37m -\033[1;32m OK !\033[1;37m"
    tput cnorm
}

